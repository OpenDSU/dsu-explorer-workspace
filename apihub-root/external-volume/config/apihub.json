{
  "storage": "../apihub-root",
  "port": 8080,
  "preventRateLimit": true,
  "activeComponents": [
    "bdns",
    "bricksFabric",
    "bricking",
    "anchoring",
    "dsu-wizard",
    "debugLogger",
    "mq",
    "secrets",
    "iframe",
    "stream",
    "versionlessDSU",
    "staticServer"
  ],
  "componentsConfig": {
    "staticServer": {
      "root": "../apihub-root/dsu-explorer"
    }
  },
  "responseHeaders": {
    "X-Frame-Options": "SAMEORIGIN",
    "X-XSS-Protection": "1; mode=block"
  },
  "enableRequestLogger": true,
  "enableJWTAuthorisation": false,
  "enableOAuth": false,
  "enableLocalhostAuthorization": false,
  "serverAuthentication": false
}
